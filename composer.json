{
    "name": "lightstep/tracer",
    "description": "LightStep instrumentation API",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "ext-bcmath": "*",
        "google/protobuf": ">=3.6.1",
        "opentracing/opentracing": "^1.0.0-beta6",
        "psr/log": "^1.0",
        "ruafozy/mersenne-twister": "^1.3"
    },
    "require-dev": {
        "phpdocumentor/phpdocumentor": "^2.8.5",
        "phpunit/phpunit": "~4.8.20"
    },
    "autoload": {
        "psr-4": {
            "LightStepBase\\": "lib"
        },
        "files": [
            "lib/LightStep.php"
        ]
    },
    "version": "1.1.0",
    "config": {
        "sort-packages": true
    }
}
